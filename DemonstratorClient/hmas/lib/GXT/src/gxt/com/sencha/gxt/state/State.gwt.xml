<module>
  <inherits name="com.sencha.gxt.widget.core.Core" />

  <!-- Create StateManager -->
  <generate-with class="com.sencha.gxt.state.rebind.StateManagerGenerator">
    <when-type-is class="com.sencha.gxt.state.client.StateManager" />
  </generate-with>
  <!-- Define property to hold autobean factory to use for state -->
  <define-configuration-property name="GXT.state.autoBeanFactory" is-multi-valued="false" />
  <set-configuration-property name="GXT.state.autoBeanFactory"
    value="com.sencha.gxt.state.client.DefaultStateAutoBeanFactory" />

</module>