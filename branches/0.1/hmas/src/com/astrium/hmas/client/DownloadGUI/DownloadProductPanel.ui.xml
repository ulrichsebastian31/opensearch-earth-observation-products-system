<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel width="450" height="800">
		<g:AbsolutePanel height="800px" width="441px">
			<g:at left="10" top="20">
				<g:TextBox width="215px" height="18px" text="product URI..." ui:field="download_product_panel_product_uri_area"/>
			</g:at>
			<g:at left="255" top="20">
				<g:Button text="Download" ui:field="download_product_panel_download_button"/>
			</g:at>
			<g:at left="244" top="684">
				<g:Button text="See response" ui:field="download_product_panel_show_response_button"/>
			</g:at>
			<g:at left="10" top="92">
				<g:HTML wordWrap="true" width="431px" height="338px" ui:field="download_product_panel_response"/>
			</g:at>
		</g:AbsolutePanel>

	</g:HTMLPanel>
</ui:UiBinder> 